<h1>ブログ一覧</h1>
<table>
  <tr>
    <th>内容</th>
  </tr>
<% @blogs.each do |blog| %>
  <tr>
    <td><%= blog.content %></td>
    <td><%= link_to 'ツイートを編集する', edit_blog_path(blog.id), data: { confirm: '本当に編集していいですか？' }  %></td>
    <td><%= link_to 'ツイートを削除する', blog_path(blog.id), method: :delete ,data: { confirm: '本当に削除していいですか？' } %></td>
  </tr>
<% end %>
</table>
<%= link_to '新しくツイートする', new_blog_path %>
